    "@item-enonic-types/lib-freemarker": "^2.0.2",
    "@item-enonic-types/lib-http-client": "^3.2.2",
    "@item-enonic-types/lib-menu": "^4.2.1",
    "@item-enonic-types/lib-time": "^1.2.1",
    "@item-enonic-types/lib-turbo-streams": "^1.1.2",
    "@itemconsulting/preset-enonic-xp": "^0.2.0",
    "@itemconsulting/xp-storybook-utils": "^0.0.7",
    "@storybook/addon-a11y": "^9.1.1",
    "@storybook/addon-links": "^9.1.1",
    "@storybook/addon-styling-webpack": "^2.0.0",
    "@storybook/addon-webpack5-compiler-swc": "^3.0.0",
    "@storybook/server-webpack5": "^9.1.1",
    "@swc/core": "^1.13.3",
    "@types/node": "^24.2.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "cssnano": "^7.1.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest-environment-jsdom": "^30.0.5",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.1",
    "postcss-loader": "^8.1.1",
    "postcss-nesting": "^13.0.2",
    "postcss-normalize": "^13.0.1",
    "postcss-reporter": "^7.1.0",
    "postcss-url": "^10.1.3",
    "prettier": "^3.6.2",
    "storybook": "^9.1.1",
    "ts-jest": "^29.4.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "@storybook/addon-docs": "^9.1.1"
  },
  "private": true,
  "scripts": {
    "build": "concurrently -c auto -g --timings npm:build:*",
    "build:assets": "node tsup/anyAssetFiles.js && npx tsup -d build/resources/main/assets",
    "build:postcss": "npx postcss src/main/resources/assets/styles/main.css -m -d build/resources/main/assets --base src/main/resources/assets --ext min.css",
    "build:server": "node tsup/anyServerFiles.js && npx tsup -d build/resources/main",
    "check": "concurrently -c auto -g --timings npm:check:types npm:lint",
    "check:types": "concurrently -g -r --timings npm:check:types:*",
    "check:types:assets": "node tsup/anyAssetFiles.js && npx tsc --noEmit -p src/main/resources/assets/tsconfig.json",
    "check:types:server": "node tsup/anyServerFiles.js && npx tsc --noEmit -p src/main/resources/tsconfig.json",
    "cov": "jest --no-cache --coverage",
    "lint": "eslint --cache \"src/main/resources/**/*.ts\"",
    "test": "jest --no-cache",
    "storybook": "storybook dev -p 6006",
    "release": "npm run build",
    "versioning": "npx changeset version && npm i --package-lock-only && NEW_VERSION=$(node -p \"require('./package.json').version\") && sed -i \"s/version.*/version = ${NEW_VERSION}/\" gradle.properties"
  }
}
