{
  "devDependencies": {
    "@enonic-types/core": "^7.14.1",
    "@enonic-types/global": "^7.14.1",
    "@enonic-types/lib-admin": "^7.14.1",
    "@enonic-types/lib-auth": "^7.14.1",
    "@enonic-types/lib-cluster": "^7.14.1",
    "@enonic-types/lib-common": "^7.14.1",
    "@enonic-types/lib-content": "^7.14.1",
    "@enonic-types/lib-context": "^7.14.1",
    "@enonic-types/lib-event": "^7.14.1",
    "@enonic-types/lib-export": "^7.14.1",
    "@enonic-types/lib-i18n": "^7.14.1",
    "@enonic-types/lib-io": "^7.14.1",
    "@enonic-types/lib-mail": "^7.14.1",
    "@enonic-types/lib-node": "^7.14.1",
    "@enonic-types/lib-portal": "^7.14.1",
    "@enonic-types/lib-project": "^7.14.1",
    "@enonic-types/lib-repo": "^7.14.1",
    "@enonic-types/lib-scheduler": "^7.14.1",
    "@enonic-types/lib-task": "^7.14.1",
    "@enonic-types/lib-websocket": "^7.14.1",
    "@eslint/js": "^9.1.1",
    "@swc/core": "^1.4.17",
    "@types/node": "^20.12.8",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  },
  "private": true,
  "scripts": {
    "build": "concurrently -c auto -g --timings npm:build:*",
    "build:assets": "find src/main/resources/assets -type f -name *.ts && npx tsup -d build/resources/main/assets || exit 0",
    "build:server": "find src/main/resources -type d -name assets -prune -o -type f -name *.ts -print && npx tsup -d build/resources/main || exit 0",
    "check": "concurrently -c auto -g --timings npm:check:types npm:lint",
    "check:types": "concurrently -g -r --timings npm:check:types:*",
    "check:types:assets": "npx tsc --noEmit -p src/main/resources/assets/tsconfig.json",
    "check:types:server": "npx tsc --noEmit -p src/main/resources/tsconfig.json",
    "cov": "jest --no-cache --coverage",
    "lint": "eslint --cache",
    "test": "jest --no-cache"
  }
}
